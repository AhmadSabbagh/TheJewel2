    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.am.engsabbagh.thejewel.UI.navigation.home.HomeFragment"

        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.am.engsabbagh.thejewel.UI.navigation.profile.ProfileFragment"

        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.am.engsabbagh.thejewel.UI.navigation.notifications.NotificationsFragment"

        tools:layout="@layout/fragment_notifications" />


    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.am.engsabbagh.thejewel.UI.navigation.cart.CartFragment"

        tools:layout="@layout/fragment_cart"/>

    <fragment
